<%- include("layout/index", tituloweb, tipo); %>

<%- include('layout/header') %>
<!-- Datos y encuestas -->
<div class="container mt-5" style="margin-bottom: 40px;">
  <div class="card border-secondary mt-2">
    <div class="card-header bg-secondary text-white text-center">
      <span>Enviar Documento</span>
    </div>
    <div class="card-body colorInfo">
      <!-- Formulario -->
      <div class="row">
        <div class="mb-3">
          <label for="formFile" class="form-label">Default file input example</label>
          <input class="form-control" type="file" id="formFile">
        </div>
      </div>

      
      <div class="d-flex justify-content-start" style="margin-top: 10px;">
        <button type="submit" class="btn" style="margin-bottom: 10px; background-color: #691532; color: #fff; margin-bottom: 10px; margin-right: 10px;">Enviar</button>
      </div>
    </div>
  </div>
</div>

<%- include("layout/footer"); %> 